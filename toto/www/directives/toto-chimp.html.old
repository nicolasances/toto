
<div class="toto-chimp" md-whiteframe="3" ng-click="openDialog()">
	<md-icon md-svg-src="images/svg/toto.svg"></md-icon>
</div>
<div id="toto-chimp-bubble" layout="column">
	<div class="bubble">Hey!</div>
	<div layout="row">
		<div flex></div>
		<md-icon md-svg-src="images/svg/arrow-down-full.svg"></md-icon>
	</div>
</div>

<div id="overlay" ng-click="hideDialog()"></div>

<div id="toto-chimp-dialog" layout="column" flex>

	<div class="toto-chimp-ai-container" layout="column">
		<div class="avatar">
			<md-icon md-svg-src="images/svg/toto.svg"></md-icon>
		</div>
		<div class="speech-container" layout="column">
			<div flex></div>
			<div class="speech">Hey, how can I help?</div>
			<div flex></div>
		</div>
	</div>
	
	<div class="info-container" flex layout="column">
	</div>
	
	<div class="user-action-container" layout="column">
		<div class="user-text">
			{{finalTranscript}}
		</div>
		<div layout="row">
			<div flex layout="column">
				<div flex></div>
				<div class="keyboard-container">
					<md-icon md-svg-src="images/svg/keyboard.svg"></md-icon>
				</div>
			</div>
			<div ng-class="['microphone-container', voiceRecognitionOn ? 'active' : '']" layout="column">
				<div flex></div>
				<md-icon md-svg-src="images/svg/microphone.svg" ng-click="toggleVoiceRecognition()"></md-icon>
				<div flex>{{recognitionStatus}}</div>
			</div>
			<div flex></div>
		</div>
	</div>


